<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <Authors>Quaverflow</Authors>
	  <Description>A library that decouples services from cross cutting concerns.</Description>
	  <Copyright>Copyright (c) Mirko Sangrigoli 2022</Copyright>
	  <PackageProjectUrl>https://github.com/Quaverflow/Howler/</PackageProjectUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageTags>AOP; Aspect Oriented Programming; Encapsulation; Abstraction;</PackageTags>
	  <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
	  <RepositoryUrl>https://github.com/Quaverflow/Howler/</RepositoryUrl>
	  <PackageIcon>icon.jpg</PackageIcon>
	  <FileVersion>1.0.0.0</FileVersion>
	  <AssemblyVersion>1.0.0.0</AssemblyVersion>
	  <Version>1.0.0</Version>
	  <PackageReadmeFile>README.md</PackageReadmeFile>
	  <PackageId>Howler.AOP</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0-preview.1.22076.8" />
  </ItemGroup>

  <ItemGroup>
    <None Update="icon.jpg">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>


</Project>
